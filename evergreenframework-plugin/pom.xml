			<version>1.0.1.RELEASE</version>
			<scope>provided</scope>
		</dependency>
		
		<dependency>
			<groupId>org.evergreen</groupId>
			<artifactId>evergreen-aop</artifactId>
			<version>1.0.1.RELEASE</version>
